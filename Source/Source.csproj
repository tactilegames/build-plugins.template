<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <Authors>Tactile Games</Authors>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>Source</PackageId>
        <Version>0.0.1-alpha.0</Version>
        <PackageProjectUrl>HTTPS_GITHUB_REPO_URL</PackageProjectUrl>
        <RepositoryUrl>HTTPS_GITHUB_REPO_URL</RepositoryUrl>
        <RootNamespace>BuildPlugins.PLUGIN_NAME</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="TactilePipeline" Version="1.0.0" />
    </ItemGroup>

</Project>
